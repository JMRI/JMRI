<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
"-//Puppy Crawl//DTD Suppressions 1.1//EN"
"http://checkstyle.sourceforge.net/dtds/suppressions_1_1.dtd">

<!--
  All suppressions in this file should also be in checkstyle-ci-suppressions.xml
-->

<suppressions>
    <!-- do not check generated code -->
    <suppress files="[/\\]target[/\\]" checks=".*"/>
    <suppress files="[/\\]java[/\\]tmp[/\\]" checks=".*"/>
    <!-- ignore private properties from Netbeans (may be empty) -->
    <suppress files="private/private.properties$" checks=".*"/>
</suppressions>
