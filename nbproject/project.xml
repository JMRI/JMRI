<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.ant.freeform</type>
    <configuration>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/1">
            <name>JMRI</name>
        </general-data>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/2">
            <!-- Do not use Project Properties customizer when editing this file manually. -->
            <name>JMRI</name>
            <properties>
                <property name="ant.script">build.xml</property>
                <property-file>${basedir}/local.properties</property-file>
                <property-file>${basedir}/release.properties</property-file>
                <property-file>${basedir}/project.properties</property-file>
            </properties>
            <folders>
                <source-folder>
                    <label>JMRI</label>
                    <location>.</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>Sources</label>
                    <type>java</type>
                    <location>java/src</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>Generated Sources</label>
                    <type>java</type>
                    <location>java/tmp</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>Unit Tests</label>
                    <type>java</type>
                    <location>java/test</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>Acceptance Tests</label>
                    <type>java</type>
                    <location>java/acceptancetest/step_definitions</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
            </folders>
            <ide-actions>
                <action name="build">
                    <script>nbproject/nbjdk.xml</script>
                    <target>debug</target>
                </action>
                <action name="clean">
                    <script>nbproject/nbjdk.xml</script>
                    <target>clean</target>
                </action>
                <action name="javadoc">
                    <script>nbproject/nbjdk.xml</script>
                    <target>javadoc</target>
                </action>
                <action name="run">
                    <script>nbproject/nbjdk.xml</script>
                    <target>panelpro</target>
                </action>
                <action name="profile">
                    <script>nbproject/nbjdk.xml</script>
                    <target>profile-nb</target>
                </action>
                <action name="rebuild">
                    <script>nbproject/nbjdk.xml</script>
                    <target>clean</target>
                    <target>debug</target>
                </action>
                <action name="debug">
                    <script>nbproject/nbjdk.xml</script>
                    <target>debug-nb</target>
                </action>
                <action name="compile.single">
                    <script>nbproject/nbjdk.xml</script>
                    <target>compile-selected-files-in-src</target>
                    <context>
                        <property>files</property>
                        <folder>${source}</folder>
                        <pattern>\.java$</pattern>
                        <format>relative-path</format>
                        <arity>
                            <separated-files>,</separated-files>
                        </arity>
                    </context>
                </action>
                <action name="compile.single">
                    <script>nbproject/nbjdk.xml</script>
                    <target>compile-selected-files-in-test</target>
                    <context>
                        <property>files</property>
                        <folder>${test}</folder>
                        <pattern>\.java$</pattern>
                        <format>relative-path</format>
                        <arity>
                            <separated-files>,</separated-files>
                        </arity>
                    </context>
                </action>
                <action name="compile.single">
                    <script>nbproject/nbjdk.xml</script>
                    <target>compile-selected-files-in-tmp</target>
                    <context>
                        <property>files</property>
                        <folder>${genjavasrcdir}</folder>
                        <pattern>\.java$</pattern>
                        <format>relative-path</format>
                        <arity>
                            <separated-files>,</separated-files>
                        </arity>
                    </context>
                </action>
                <action name="test">
                    <script>nbproject/nbjdk.xml</script>
                    <target>ci-test</target>
                </action>
                <action name="test.single">
                    <script>nbproject/nbjdk.xml</script>
                    <target>test-single</target>
                    <context>
                        <property>test.includes</property>
                        <folder>${source}</folder>
                        <pattern>\.java$</pattern>
                        <format>java-name</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                    </context>
                </action>
                <action name="test.single">
                    <script>nbproject/nbjdk.xml</script>
                    <target>test-single</target>
                    <context>
                        <property>test.includes</property>
                        <folder>${test}</folder>
                        <pattern>\.java$</pattern>
                        <format>java-name</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                    </context>
                </action>
                <action name="run.single">
                    <script>nbproject/nbjdk.xml</script>
                    <target>test-single</target>
                    <context>
                        <property>test.includes</property>
                        <folder>${test}</folder>
                        <pattern>\.java$</pattern>
                        <format>java-name</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                    </context>
                </action>
                <action name="debug.single">
                    <script>nbproject/nbjdk.xml</script>
                    <target>debug-test-single.nb</target>
                    <context>
                        <property>test.includes</property>
                        <folder>${test}</folder>
                        <pattern>\.java$</pattern>
                        <format>java-name</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                    </context>
                </action>
                <action name="debug.fix">
                    <script>nbproject/nbjdk.xml</script>
                    <target>debug-apply-fix-in-src</target>
                    <context>
                        <property>fix.file</property>
                        <!--<folder>${src.dir}</folder>-->
                        <folder>${source}</folder>
                        <pattern>\.java$</pattern>
                        <format>relative-path-noext</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                  </context>
                </action>            
                <action name="debug.fix">
                    <script>nbproject/nbjdk.xml</script>
                    <target>debug-apply-fix-in-test</target>
                    <context>
                        <property>fix.file</property>
                        <folder>${test}</folder>
                        <pattern>\.java$</pattern>
                        <format>relative-path-noext</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                  </context>
                </action>            
                <action name="debug.fix">
                    <script>nbproject/nbjdk.xml</script>
                    <target>debug-apply-fix-in-tmp</target>
                    <context>
                        <property>fix.file</property>
                        <!--<folder>${src.dir}</folder>-->
                        <folder>${genjavasrcdir}</folder>
                        <pattern>\.java$</pattern>
                        <format>relative-path-noext</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                  </context>
                </action>            
            </ide-actions>
            <export>
                <type>folder</type>
                <location>target/classes</location>
                <script>nbproject/nbjdk.xml</script>
                <build-target>debug</build-target>
            </export>
            <export>
                <type>folder</type>
                <location>target/classes</location>
                <script>nbproject/nbjdk.xml</script>
                <build-target>debug</build-target>
            </export>
            <view>
                <items>
                    <source-folder style="packages">
                        <label>Sources</label>
                        <location>java/src</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>Generated Sources</label>
                        <location>java/tmp</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>Unit Tests</label>
                        <location>java/test</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>Acceptance Tests</label>
                        <location>java/acceptancetest/step_definitions</location>
                    </source-folder>
                    <source-file>
                        <location>${ant.script}</location>
                    </source-file>
                </items>
                <context-menu>
                    <ide-action name="build"/>
                    <ide-action name="rebuild"/>
                    <ide-action name="clean"/>
                    <ide-action name="javadoc"/>
                    <ide-action name="run"/>
                    <ide-action name="test"/>
                    <ide-action name="profile"/>
                    <ide-action name="debug"/>
                    <action>
                        <script>${ant.script}</script>
                        <label>Make Jar File</label>
                        <target>jar</target>
                    </action>
                    <action>
                        <script>${ant.script}</script>
                        <label>Run PanelPro</label>
                        <target>panelpro</target>
                    </action>
                    <action>
                        <script>${ant.script}</script>
                        <label>Run DecoderPro</label>
                        <target>decoderpro</target>
                    </action>
                    <action>
                        <script>${ant.script}</script>
                        <label>Run Remote DecoderPro</label>
                        <target>decoderpro-remote</target>
                    </action>
                    <action>
                        <script>${ant.script}</script>
                        <label>Run Remote PanelPro</label>
                        <target>panelpro-remote</target>
                    </action>
                </context-menu>
            </view>
            <subprojects/>
        </general-data>
