    "/app/node_modules/angular/angular.min.js",
    "/app/node_modules/angular-resource/angular-resource.min.js",
    "/app/node_modules/angular-route/angular-route.min.js",
    "/app/node_modules/angular-patternfly/node_modules/angular-sanitize/angular-sanitize.min.js",
    "/app/node_modules/angular-patternfly/node_modules/angular-ui-bootstrap/ui-bootstrap.min.js",
    "/app/node_modules/angular-patternfly/node_modules/angular-ui-bootstrap/ui-bootstrap-tpls.min.js",
    "/app/node_modules/angular-patternfly/dist/angular-patternfly.min.js",
    "/app/node_modules/angular-patternfly/node_modules/angular-drag-and-drop-lists/angular-drag-and-drop-lists.min.js",
