<!-- A very basic programmer, containing only the variables that -->
<!-- register-mode decoders have. -->

<programmer decoderFilePanes="no">

	<pane name="Basic" nameFmt="item">
		<column>

	 		<display item="Short Address" label="Short Address"/>
			<label label=" "/>

			<display item="Analog (DC) Operation"/>
			<display item="Speed Step Mode"/>
			<display item="Locomotive Direction"/>
			<label label=" "/>

	 		<display item="Vstart"/>
			<label label=" "/>

	 		<display item="Accel"/>
			<label label=" "/>

	 		<display item="Decel"/>
			<label label=" "/>

		</column>
	</pane>

</programmer>
</programmer-config>
