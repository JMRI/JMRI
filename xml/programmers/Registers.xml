<programmer-config xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://jmri.org/xml/schema/programmer.xsd">
  <version author="Bob Jacobsen" version="2" lastUpdated="20020112"/>
  <!-- A very basic programmer, containing only the variables that -->
  <!-- register-mode decoders have. -->
  <programmer decoderFilePanes="no" showEmptyPanes="default" showFnLanelPane="no" showRosterMediaPane="no">
    <pane nameFmt="item">
      <name>Basic</name>
      <column>
        <display item="Short Address" label="Short Address"/>
        <label>
          <text> </text>
        </label>
        <display item="Analog (DC) Operation"/>
        <display item="Speed Step Mode"/>
        <display item="Locomotive Direction"/>
        <label>
          <text> </text>
        </label>
        <display item="Vstart"/>
        <label>
          <text> </text>
        </label>
        <display item="Accel"/>
        <label>
          <text> </text>
        </label>
        <display item="Decel"/>
        <label>
          <text> </text>
        </label>
      </column>
      <name xml:lang="it">Basic</name>
	  <name xml:lang="ca">Bàsic</name>
      <name xml:lang="cs">Základní</name>
    </pane>
  </programmer>
</programmer-config>
