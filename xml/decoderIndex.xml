<!-- The very first decoder entry becomes the default, so leave it an NMRA one -->
<!-- The first entry in each family should be the highest-function model, as   -->
<!-- that's the one that will appear at the top of the selection box           -->

<familyList>
	<family name="NMRA" file="NMRA.xml" mfg="NMRA">
		<decoder model="All CVs" comment="List is from RP 9.2.2" />
	</family>

	<family name="Atlas" file="Atlas_DualMode.xml" mfg="Atlas">
		<decoder model="Dual Mode" comment="Mfg code is from Lenz, not Atlas" numOuts="2" />
	</family>

	<family name="Digitrax 1x2" file="Digitrax_1x2.xml" mfg="Digitrax" lowVersionID="240" highVersionID="242">
		<decoder model="DH142" numOuts="4" />
		<decoder model="DN142" numOuts="4" />
	</family>

	<family name="Digitrax 1x1" file="Digitrax_1x1.xml" mfg="Digitrax" lowVersionID="33" highVersionID="45">
		<decoder model="DH121" numOuts="2" />
		<decoder model="DN140" numOuts="4" />
	</family>
			
	<family name="Digitrax 150" file="Digitrax_150.xml" mfg="Digitrax" lowVersionID="107">
		<decoder model="DH150" numOuts="2" />
	</family>

	<family name="D13SR Family" file="NCE_D13SR.xml" mfg="North Coast Engineering">
		<decoder model="D13SR" numOuts="3" />
	</family>

</familyList>

</decoderIndex>
