            </column>
        </row>
        <row>
            <label>
                <text> </text>
            </label>
        </row>
        <row>
            <display item="id" layout="above" label="CAN ID" tooltip="CBUS CAN Id - use with care" />
        </row>
        <row>
            <label>
                <text> </text>
            </label>
        </row>
        <row>
            <display item="nodeNumber" layout="above" label="Node Number" tooltip="CBUS Node Number - use with care" />
        </row>
    </column>
