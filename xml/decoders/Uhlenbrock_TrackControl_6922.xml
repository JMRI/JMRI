              <label xml:lang="fr">Version: </label>
              <label xml:lang="de">Softwareversion: </label>
              <label xml:lang="nl">Firmware-versie: </label>
              <label>Firmware Version: </label>
            </variable>

            <variable CV="6922.3" item="Configuration" default="64">
              <decVal/>
