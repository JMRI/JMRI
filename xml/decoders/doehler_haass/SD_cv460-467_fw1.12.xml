      <choice xml:lang="nl">In blenden-</choice>
    </enumChoice>
    <enumChoice choice="Fade out">
      <choice>Fade out</choice>
      <choice xml:lang="de">Ausblenden</choice>
      <choice xml:lang="nl">Uit blenden</choice>
