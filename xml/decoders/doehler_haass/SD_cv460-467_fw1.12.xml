      <choice xml:lang="nl">Gyralight</choice>
    </enumChoice>
    <enumChoice choice="strobe">
      <choice>strobe</choice>
      <choice xml:lang="de">Blitz</choice>
      <choice xml:lang="nl">Flitz</choice>
    </enumChoice>
    <enumChoice choice="double strobe">
      <choice>double strobe</choice>
      <choice xml:lang="de">Doppel Blitz</choice>
      <choice xml:lang="nl">Dubbele flitz</choice>
    </enumChoice>
    <enumChoice choice="-">
      <choice>Dynamic smoke generator,</choice>
      <choice xml:lang="de">Dynamicher Raucherzeuger</choice>
      <choice xml:lang="nl">dynamische rookgenerator</choice>
    </enumChoice>
    <enumChoice choice="Firebox (red)">
      <choice>Firebox (red)</choice>
      <choice xml:lang="de">Feuerbüchse (rot)</choice>
      <choice xml:lang="nl">Vuurkist (rood)</choice>
    </enumChoice>
    <enumChoice choice="-">
      <choice>Firebox (orange)</choice>
