<!-- Switching mode                                                         -->
<enumVal xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://jmri.org/xml/schema/decoder.xsd">
  <enumChoice value="0">
    <choice>either</choice>
    <choice xml:lang="de">entweder</choice>
    <choice xml:lang="nl">beide</choice>
  </enumChoice>
  <enumChoice value="2">
    <choice>from S1 to S2</choice>
    <choice xml:lang="de">von S1 nach S2</choice>
    <choice xml:lang="nl">van S1 naar S2</choice>
  </enumChoice>
  <enumChoice value="3">
    <choice>from S2 to S1</choice>
    <choice xml:lang="de">von S2 nach S1</choice>
    <choice xml:lang="nl">van S2 naar S1</choice>
  </enumChoice>
</enumVal>
