  <!-- 	Uhlenbrock Decoder CV 14									-->
  <!-- 	valid for certain decoder types only  :              		-->    
  <!-- 		73105, 73115, 73145, 73405, 73415 (not exhaustive)   	-->
  </revhistory>
    <variable item="Analog Mode Function Status - F0" CV="14" default="1" mask="XXXXXXXV">
        <xi:include href="http://jmri.org/xml/decoders/parts/enum-OffOn.xml"/>
        <label>Function in Analog - Lights</label>
        <label xml:lang="de">Funktion im Analogbetrieb - Licht</label>
        <label xml:lang="nl">Functie bij Analoogbedrijf - Licht</label>
    </variable>
    <!-- bits 1-3 not used -->
    <variable item="Analog Mode Function Status - F9" CV="14" default="0" mask="XXXVXXXX" minOut="11">
        <xi:include href="http://jmri.org/xml/decoders/parts/enum-OffOn.xml"/>
        <label>Function in Analog - A9</label>
        <label xml:lang="de">Funktion im Analogbetrieb - A9</label>
        <label xml:lang="nl">Functie bij Analoogbedrijf - A9</label>
    </variable>
    <variable item="Analog Mode Function Status - F10" CV="14" default="0" mask="XXVXXXXX" minOut="12">
        <xi:include href="http://jmri.org/xml/decoders/parts/enum-OffOn.xml"/>
        <label>Function in Analog - A10</label>
        <label xml:lang="de">Funktion im Analogbetrieb - A10</label>
        <label xml:lang="nl">Functie bij Analoogbedrijf - A10</label>
    </variable>
    <variable item="Analog Mode Function Status - F11" CV="14" default="0" mask="XVXXXXXX" minOut="13">
        <xi:include href="http://jmri.org/xml/decoders/parts/enum-OffOn.xml"/>
        <label>Function in Analog - A11</label>
        <label xml:lang="de">Funktion im Analogbetrieb - A11</label>
        <label xml:lang="nl">Functie bij Analoogbedrijf - A11</label>
    </variable>
    <variable item="Analog Mode Function Status - F12" CV="14" default="0" mask="VXXXXXXX" minOut="14">
        <xi:include href="http://jmri.org/xml/decoders/parts/enum-OffOn.xml"/>
        <label>Function in Analog - A12</label>
        <label xml:lang="de">Funktion im Analogbetrieb - A12</label>
        <label xml:lang="nl">Functie bij Analoogbedrijf - A12</label>
    </variable>
</variables>
