    <variable CV="59" item="Sound Setting 6" default="0">
        <decVal/>
        <label>Sound at Change of Direction</label>
        <label xml:lang="de">Sound bei Richtungswechsel</label>
        <label xml:lang="fr">Son au changement de sens</label>
        <tooltip>&lt;html&gt;Volume of Sound at change of direction&lt;br&gt;0 = no sound at change of direction&lt;/html&gt;</tooltip>
        <tooltip xml:lang="de">&lt;html&gt;Lautstärke Sound bei Richtungswechsel&lt;br&gt;0 = kein Sound bei Richtungswechsel&lt;/html&gt;</tooltip>
        <tooltip xml:lang="fr">&lt;html&gt;Volume du son au changement de sens&lt;br&gt;0 = pas de son au changement de sens&lt;/html&gt;</tooltip>
    </variable>
