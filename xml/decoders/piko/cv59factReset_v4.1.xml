	<factReset label="Reset CVs 0 - 256" CV="59" default="1">
		<label xml:lang="it">Reset CV 0 - 256 ai valori di fabbrica</label>
		<label xml:lang="nl">Zet CV's 0 - 256 terug naar fabrieksinstelling</label>
		<label xml:lang="de">Reset CV    0 - 256 (Werkseinstellungen &amp; RailCom Bank 7)</label>
		<label xml:lang="cs">Reset CV    0 - 256 do továrního nastavení</label>
	</factReset>
	<factReset label="Reset CV 257 - 512 (RailCom Plus® Banks 5 &amp; 6)" CV="59" default="2">
		<label xml:lang="de">Reset CV 257 - 512 (RailCom Plus® Banken 5 &amp; 6)</label>
		<label xml:lang="cs">Reset CV 257 - 512 (RailCom Plus® Banky 5 &amp; 6)</label>
	</factReset>
	<factReset label="Reset CV 257 - 512 (extended Function Mapping Banks 1 &amp; 2)" CV="59" default="3">
		<label xml:lang="de">Reset CV 257 - 512 (erweitertes Function Mapping Banken 1 &amp; 2)</label>
		<label xml:lang="cs">Reset CV 257 - 512 (Rozšířené mapování funkcí Banky 1 &amp; 2)</label>
	</factReset>
	<factReset label="Reset CV 257 - 512 (PWM-Modulation Funktions outputs Banks 3 &amp; 4)" CV="59" default="4">
		<label xml:lang="de">Reset CV 257 - 512 (PWM-Modulation Funktionsausgänge Banken 3 &amp; 4)</label>
		<label xml:lang="cs">Reset CV 257 - 512 (PWM Modulace funkční výstupy Banky 3 &amp; 4)</label>
	</factReset>
	<factReset label="Reset CV 900 - 939 (SUSI Sound Banks 1 - 8)" CV="905,1021=0" default="1">
		<label xml:lang="de">Reset CV 900 - 939 (SUSI-Soundbänke 1 - 8)</label>
		<label xml:lang="cs">Reset CV 900 - 939 (SUSI-Sound Banky 1 - 8)</label>
	</factReset>
