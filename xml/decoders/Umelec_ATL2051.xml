<decoder-config xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://jmri.org/xml/schema/decoder.xsd">
	<version author="jefflaw@datamasta.co.nz" version="1.00" lastUpdated="20040809"/>
	<version author="W.D.Kok" version="2.00" lastUpdated="20180217"/>
	<!-- version 1.00 - initial version                                          -->
	<!-- Version 2.00 - CV Corrections and German and Dutch translating inserted -->
	<decoder>
		<family name="Umelec ATLPlus Engine Module" mfg="Umelec Ing Buero">
			<model model="ATL2051" lowVersionID="57" highVersionID="59" comment="For DC and AC Motors with Infrared Light Barrier" maxInputVolts="21V" maxMotorCurrent="1.5A" maxTotalCurrent="1.5A" formFactor="H0/0" connector="Wires" numOuts="2" numFns="4" productID="ModelA">
			<size length="25" width="13.5" height="5.5" units="mm"/>
			</model>
			<model model="ATL2054" lowVersionID="17" highVersionID="19" comment="For DC Motors, includes EMP control" maxInputVolts="21V" maxMotorCurrent="1.5A" maxTotalCurrent="1.5A" formFactor="H0/0" connector="Wires/NEM652" numOuts="2" numFns="4" productID="ModelB">
			<size length="25" width="12.5" height="5" units="mm"/>
			</model>
		</family>
		<programming direct="yes" paged="yes" register="yes" ops="yes"/>
		<variables>
			<xi:include href="http://jmri.org/xml/decoders/nmra/shortAndLongAddress.xml"/>
			<variable CV="2" item="Vstart" comment="Range 0-15"  default="8">
				<decVal max="15"/>
				<label>Start value (0-15)</label>
				<label xml:lang="it">Volt Partenza (0-15)</label>
				<label xml:lang="fr">V démarr. (0-15)</label>
				<label xml:lang="de">Startspannung (0-15)</label>
				<label xml:lang="nl">Start spanning (0-15)</label>
				<comment>Range 0-15</comment>
			</variable>
			<variable CV="3" item="Accel Normal" mask="XXXXVVVV" default="4">
				<decVal min="0" max="15"/>
				<label>Acceleration</label><!--Normal Mode-->
				<label xml:lang="it">Accellerazione (0-15)</label>
				<label xml:lang="fr">Accelération (0-15)</label>
				<label xml:lang="de">Anfahrverzögerung (015)</label>
				<label xml:lang="nl">Acceleratie tijd (0-15)</label>
			</variable>
			<variable CV="3" item="Accel Signal" mask="VVVVXXXX" default="4">
				<decVal min="0" max="15"/>
				<label>Acceleration</label><!--Signal Mode-->
				<label xml:lang="it">Accellerazione (0-15)</label>
				<label xml:lang="fr">Accelération (0-15)</label>
				<label xml:lang="de">Beschleunigung zeit (0-15)</label>
				<label xml:lang="nl">Acceleratie tijd (0-15)</label>
			</variable>
			<variable CV="4" item="Decel Normal" mask="XXXXVVVV" default="4">
				<decVal min="0" max="15"/>
				<label>Deceleration</label><!--Normal Mode-->
				<label xml:lang="it">Decellerazione (0-15)</label>
				<label xml:lang="fr">Décélération (0-15)</label>
				<label xml:lang="de">Bremszeit (0-15)</label>
				<label xml:lang="nl">Rem tijd (0-15)</label>
			</variable>
			<variable CV="4" item="Decel Signal" mask="VVVVXXXX" default="4">
				<decVal min="0" max="15"/>
				<label>Deceleration</label><!--Signal Mode-->
				<label xml:lang="it">Decellerazione (0-15)</label>
				<label xml:lang="fr">Décélération (0-15)</label>
				<label xml:lang="de">Bremszeit (0-15)</label>
				<label xml:lang="nl">Rem tijd (0-15)</label>
			</variable>
			<!-- ATL CV -->
			<variable CV="5" item="ATL-A" mask="XXXXXXXV"> <!-- ++ mode -->
				<enumVal>
					<enumChoice choice="Speed Step Mode">
						<choice>Speed Step Mode</choice>
						<choice xml:lang="de">++Modus Aus</choice>
						<choice xml:lang="nl">++Modus uit</choice>
					</enumChoice>
					<enumChoice choice="++Mode">
						<choice>++Mode</choice>
						<choice xml:lang="de">++Modus Ein</choice>
						<choice xml:lang="nl">++Modus aan</choice>
					</enumChoice>
				</enumVal>
				<label>Speed Control Mode</label>
				<label xml:lang="de">Spezielle Betriebsart</label>
				<label xml:lang="nl">Speciale modus</label>
			</variable>
			<variable CV="5" item="ATL-B" mask="XXXXXXVX"> <!-- Shuttle mode -->
				<enumVal>
					<enumChoice choice="No Shuttle">
						<choice>No Shuttle</choice>
						<choice xml:lang="de">Pendelbetrieb Aus</choice>
						<choice xml:lang="nl">Pendelautomaat uit</choice>
					</enumChoice>
					<enumChoice choice="Shuttle Operation">
						<choice>Shuttle Operation</choice>
						<choice xml:lang="de">Pendelbetrieb Ein</choice>
						<choice xml:lang="nl">Pendelautomaat aan</choice>
					</enumChoice>
				</enumVal>
				<label>Shuttle Operation</label>
				<label xml:lang="de">Pendelbetrieb</label>
				<label xml:lang="nl">Pendeldienst</label>
			</variable>
			<variable CV="5" item="ATL-C" mask="XXXXXVXX"> <!-- Signal mode -->
				<xi:include href="http://jmri.org/xml/decoders/parts/enum-OnOff.xml"/>
				<label>Signal mode</label>
				<label xml:lang="de">Signal Modus</label>
				<label xml:lang="nl">Signaal modus</label>
			</variable>
			<variable CV="5" item="ATL-D" mask="XXXXVXXX"> <!-- Whistle mode -->
				<enumVal>
					<enumChoice choice="Lanier brake">
						<choice>One tone whistle</choice>
						<choice xml:lang="de">Ein Ton Pfeife</choice>
						<choice xml:lang="nl">Één ton fluit</choice>
					</enumChoice>
					<enumChoice choice="Braking curve">
						<choice>Two-tone whistle</choice>
						<choice xml:lang="de">Zwei Ton Pfeife</choice>
						<choice xml:lang="nl">Twee ton fluit</choice>
					</enumChoice>
				</enumVal>
				<label>Whistle mode</label>
				<label xml:lang="de">Pfeife Modus</label>
				<label xml:lang="nl">Fluit modus</label>
			</variable>      
			<variable CV="5" item="ATL-E" mask="XXXVXXXX"> <!-- Sound Style -->
				<enumVal>
					<enumChoice choice="Steam">
				<choice>Steam</choice>
				<choice xml:lang="de">Dampf</choice>
				<choice xml:lang="nl">Stoom</choice>
					</enumChoice>
					<enumChoice choice="Diesel">
						<choice>Diesel</choice>
					</enumChoice>
				</enumVal>
				<label>Sound Style</label>
				<label xml:lang="de">Sound type</label>
				<label xml:lang="nl">Geluids type</label>
			</variable>
			<variable CV="5" item="ATL-F" mask="XXVXXXXX"> <!-- Exhaust Beat Control -->
				<enumVal>
					<enumChoice choice="Internal">
						<choice>Internal</choice>
						<choice xml:lang="de">Intern</choice>
						<choice xml:lang="nl">Interne</choice>
					</enumChoice>
					<enumChoice choice="External e.g. Cam">
						<choice>External e.g. Cam</choice>
						<choice xml:lang="de">Extern</choice>
						<choice xml:lang="nl">Externe</choice>
					</enumChoice>
				</enumVal>
				<label>Exhaust Beat Control</label>
				<label xml:lang="de">Dampftrigger</label>
				<label xml:lang="nl">Stoom slag controle</label>
			</variable>
			<variable CV="5" item="ATL-G" mask="XVXXXXXX"> <!-- module input -->
				<enumVal>
					<enumChoice choice="Remote Only">
						<choice>Remote Only</choice>
						<choice xml:lang="de">Nur Pendelfunktion</choice>
						<choice xml:lang="nl">Pendelbetrieb</choice>
					</enumChoice>
					<enumChoice choice="Remote and Reed Switch">
						<choice>Remote and Reed Switch</choice>
						<choice xml:lang="de">Pendelfunktion und Lokpfeife</choice>
						<choice xml:lang="nl">Pendelbetrieb en Lokpfeife</choice>
					</enumChoice>
				</enumVal>
				<label>Switches the module input</label>
				<label xml:lang="de">Schaltet den Moduleingang</label>
				<label xml:lang="nl">Schakelt de module-ingang</label>
			</variable>
			<variable CV="5" item="ATL-H" mask="VXXXXXXX" include="ModelA"> <!-- Motor EMP -->
				<enumVal>
					<enumChoice choice="Brake">
						<choice>Normal pulse rate to motor</choice>
						<choice xml:lang="de">Normale Puls-rate zum Motor</choice>
						<choice xml:lang="nl">Normale puls frequents naar de motor</choice>
					</enumChoice>
					<enumChoice choice="Stop">
						<choice>quadrupled pulse rate to the motor</choice>
						<choice xml:lang="de">Vervierfachte Puls rate zum Motor</choice>
						<choice xml:lang="nl">verviervoudigde puls frequents naar de motor</choice>
					</enumChoice>
				</enumVal>
				<label>Motor EMP, Optimises the speed regulator</label>
				<label xml:lang="de">Motor EMP, optimiert den Drehzahlregler</label>
				<label xml:lang="nl">Motor EMP, optimaliseert de snelheidsregelaar</label>
			</variable>
			<variable CV="5" item="ATL-H" mask="VXXXXXXX" include="ModelB"> <!-- Motor EMP -->
				<enumVal>
					<enumChoice choice="Brake">
						<choice>Normal pulse rate to motor</choice>
						<choice xml:lang="de">Normale Puls-rate zum Motor</choice>
						<choice xml:lang="nl">Normale puls frequents naar de motor</choice>
					</enumChoice>
					<enumChoice choice="Stop">
						<choice>Half pulse rate to motor</choice>
						<choice xml:lang="de">Halbe Pulsrate zum Motor</choice>
						<choice xml:lang="nl">Halveerde puls frequents naar de motor</choice>
					</enumChoice>
				</enumVal>
				<label>Motor EMP, Optimises the speed regulator</label>
				<label xml:lang="de">Motor EMP, optimiert den Drehzahlregler</label>
				<label xml:lang="nl">Motor EMP, optimaliseert de snelheidsregelaar</label>
			</variable>
			<variable CV="6" item="Vmax Signal Mode" default="40" comment="Range 30-63">
				<decVal min="30" max="63"/>
				<label>Run Speed</label>
				<label xml:lang="de">Signal-Geschwindigkeit</label>
				<label xml:lang="nl">Signaal snelheid</label>
				<comment>Range 30-63</comment>
			</variable>
			<variable CV="7" item="Decoder Version" readOnly="yes" default="59" include="ModelA">
				<decVal/>
				<label>Decoder Version No</label>
				<label xml:lang="it">Versione Decoder: </label>
				<label xml:lang="fr">Version décodeur: </label>
				<label xml:lang="de">Decoder Version: </label>
				<label xml:lang="nl">Decoder versie: </label>				
			</variable>
			<variable CV="7" item="Decoder Version" readOnly="yes" default="17" include="ModelB">
				<decVal/>
				<label>Decoder Version No</label>
				<label xml:lang="it">Versione Decoder: </label>
				<label xml:lang="fr">Version décodeur: </label>
				<label xml:lang="de">Decoder Version: </label>
				<label xml:lang="nl">Decoder versie: </label>
			</variable>
			<variable CV="8" item="Manufacturer"    readOnly="yes" default="147">
				<decVal/>
				<label>Manufacturer ID</label>
				<label xml:lang="it">ID Costruttore: </label>
				<label xml:lang="fr">ID constructeur: </label>
				<label xml:lang="de">Hersteller ID: </label>
				<label xml:lang="nl">Fabrikanten ID: </label>
			</variable>
			<variable CV="9" item="Total PWM Period ATL2051"    default="17"  comment="Range 0-250, default is 17  on ATL2051" include="ModelA">
				<decVal/>
				<label>Total PWM Period</label>
				<label xml:lang="it">Periodo PWM (Frequenza pilotaggio Motore)</label>
				<label xml:lang="de">Periode PWM (Pulsweitenmodulation der Motoransteuerung)</label>
				<label xml:lang="nl">Periode PWM (Pulsbreedtemodulatie der Motoraansteuering)</label>
				<comment>Range 0-250, default is 17 on ATL2051</comment>
				</variable>
			<variable CV="9" item="Total PWM Period ATL2054"    default="240" comment="Range 0-250, default is 240 on ATL2054" include="ModelB">
				<decVal/>
				<label>Total PWM Period</label>
				<label xml:lang="it">Periodo PWM (Frequenza pilotaggio Motore)</label>
				<label xml:lang="de">Periode PWM (Pulsweitenmodulation der Motoransteuerung)</label>
				<label xml:lang="nl">Periode PWM (Pulsbreedtemodulatie der Motoraansteuering)</label>
				<comment>Range 0-250, default is 240 on ATL2054</comment>
			</variable>
			<variable CV="10" item="Vmax ++Mode"                default="48"  comment="Range 30-63">
				<decVal/>
				<label>Vmax ++Mode</label>
				<label xml:lang="de">Vmax ++Modus</label>
				<label xml:lang="nl">Vmax ++Modus</label>
				<comment>Range 30-63</comment>
			</variable>
			<variable CV="11" item="Sound Group 1 Option 1"     default="64"  comment="Range 0-255, default is 64  on ATL2051" include="ModelA">
				<decVal/>
					<label>Steam Divider per wheel turn</label>
					<label xml:lang="de">Dampftrigger</label>
					<label xml:lang="nl">Stoom deler per wiel omdraaiing</label>
					<comment>Range 0-255, default is 64 on ATL2051</comment>
			</variable>
			<variable CV="11" item="Sound Group 1 Option 2"     default="127" comment="Range 0-255, default is 127 on ATL2054" include="ModelB">			
				<decVal/>
					<label>Steam Divider per wheel turn</label>
					<label xml:lang="de">Dampftrigger</label>
					<label xml:lang="nl">Stoom deler per wiel omdraaiing</label>
					<comment>Range 0-255, default is 127 on ATL2054</comment>
			</variable>
			<variable CV="12" item="Sound Group 2 Option 1"     default="44"  comment="Range 44-63">
				<decVal min="44" max="63"/>
				<label>Whistle Tone Start Pointer 1 </label>
				<label xml:lang="de">Pfeifton Start punkt 1 </label>
				<label xml:lang="nl">Fluittoon start punt 1 </label>
				<comment>Range 44-63</comment>
			</variable>
			<variable CV="13" item="Sound Group 2 Option 2"     default="44"  comment="Range 44-63">
				<decVal min="44" max="63"/>
				<label>Whistle Tone Start Pointer 2 </label>
				<label xml:lang="de">Pfeifton Start punkt 2 </label>
				<label xml:lang="nl">Fluittoon start punt 2 </label>
				<comment>Range 44-63</comment>
			</variable>
			<variable CV="14" item="Function 1" mask="XXXXXXVV" default="0"   include="ModelB"> <!-- Function output C -->
				<enumVal>
					<enumChoice choice="F1">
						<choice> F1 </choice>
					</enumChoice>
					<enumChoice choice="F2">
						<choice> F2 </choice>
					</enumChoice>
					<enumChoice choice="F3">
						<choice> F3 </choice>
					</enumChoice>
					<enumChoice choice="F4">
						<choice> F4 </choice>
					</enumChoice>
				</enumVal>
				<label>Function output C (FET) on </label>
				<label xml:lang="de">Funktion Ausgang C (FET) auf </label>
				<label xml:lang="nl">Functie uitgang C (FET) op </label>
			</variable>
			<variable CV="14" item="Function 2" mask="XXXVVXXX" default="1"   include="ModelB"> <!-- Function output D -->
				<enumVal>
					<enumChoice choice="F1">
						<choice> F1 </choice>
					</enumChoice>
					<enumChoice choice="F2">
						<choice> F2 </choice>
					</enumChoice>
					<enumChoice choice="F3">
						<choice> F3 </choice>
					</enumChoice>
					<enumChoice choice="F4">
						<choice> F4 </choice>
					</enumChoice>
				</enumVal>
				<label>Function output D (CMOS) on </label>
				<label xml:lang="de">Funktion Ausgang D (CMOS) auf </label>
				<label xml:lang="nl">Functie uitgang D (CMOS) op </label>
			</variable>
			<variable CV="14" item="Function 3" mask="XVVXXXXX" default="0"   include="ModelB"> <!-- Function output D Blink/Whistle/output -->
				<enumVal>
					<enumChoice choice="off">
						<choice> - </choice>
					</enumChoice>
					<enumChoice choice="blink/flash">
						<choice> blink/flash </choice>
						<choice xml:lang="de"> Blinken/Blitzen </choice>
						<choice xml:lang="nl">Knipperen/Flitsend </choice>
					</enumChoice>
					<enumChoice choice="whistle">
						<choice> whistle </choice>
						<choice xml:lang="de"> Pfeife </choice>
						<choice xml:lang="nl">Fluit </choice>
					</enumChoice>
				</enumVal>
				<label>Function output D </label>
				<label xml:lang="de">Funktion Ausgang D </label>
				<label xml:lang="nl">Functie uitgang D </label>
			</variable>
			<variable CV="14" item="Function 4" mask="VXXXXXXX" default="0"   include="ModelB"> <!-- Main switch -->
				<enumVal>
					<enumChoice choice="On">
						<choice> On </choice>
						<choice xml:lang="de"> Ein </choice>
						<choice xml:lang="nl"> Aan </choice>
					</enumChoice>
					<enumChoice choice="Off">
						<choice> Off </choice>
						<choice xml:lang="de"> Aus </choice>
						<choice xml:lang="nl"> Uit </choice>
					</enumChoice>
				</enumVal>
				<label>Main switch on F4</label>
				<label xml:lang="de">Hauptschalter auf F4 </label>
				<label xml:lang="nl">Hoofdschakelaar op F4 </label>
			</variable>
			<variable CV="15" item="Period"     mask="XXXXXVVV" default="1"   include="ModelB"> <!-- Blink/flash generator Period -->
				<enumVal>
					<enumChoice choice="-">
						<choice> - </choice>
					</enumChoice>
					<enumChoice choice="1800">
						<choice> 1800 ms </choice>
					</enumChoice>
					<enumChoice choice="900">
						<choice> 900 ms </choice>
					</enumChoice>
					<enumChoice choice="600">
						<choice> 600 ms </choice>
					</enumChoice>
					<enumChoice choice="450">
						<choice> 450 ms </choice>
					</enumChoice>
					<enumChoice choice="360">
						<choice> 360 ms </choice>
					</enumChoice>
					<enumChoice choice="300">
						<choice> 300 ms </choice>
					</enumChoice>
					<enumChoice choice="250">
						<choice> 250 ms </choice>
					</enumChoice>
				</enumVal>
				<label>Period </label>
				<label xml:lang="de">Zeitraum </label>
				<label xml:lang="nl">Periode </label>
			</variable>
			<variable CV="15" item="Duty cycle" mask="VVVVVXXX" default="16"  include="ModelB"> <!-- Blink/flash generator Duty cycle-->
				<enumVal>	
					<enumChoice choice="3%" value = "1">
						<choice> 3% </choice>
					</enumChoice>
					<enumChoice choice="6%" value = "2">
						<choice> 6% </choice>
					</enumChoice>
					<enumChoice choice="12%" value = "4">
						<choice> 12% </choice>
					</enumChoice>
					<enumChoice choice="25%" value = "8">
						<choice> 25% </choice>
					</enumChoice>
					<enumChoice choice="50%" value = "16">
						<choice> 50% </choice>
					</enumChoice>
					<enumChoice choice="53%" value = "17">
						<choice> 53% </choice>
					</enumChoice>
					<enumChoice choice="75%" value = "24">
						<choice> 75% </choice>
					</enumChoice>
				</enumVal>
				<label>Duty cycle </label>
				<label xml:lang="de">Einschaltdauer </label>
				<label xml:lang="nl">Inschakeltijd </label>
			</variable>
			<variable CV="16" item="Operation byte" readOnly="yes" default="160">
				<decVal min="160" max="160"/>
				<label>Operation byte</label>
				<label xml:lang="de">Betriebs Byte</label>
				<label xml:lang="nl">Werking byte</label>
				<comment>Range 160</comment>
			</variable>
			<!-- CV=19 -->
			<variable CV="19" item="ConsistAddress"         mask="XVVVVVVV" default="0">
				<decVal min="0" max="127"/>
				<label>Advanced Consist Address</label>
				<label xml:lang="it">Indirizzo Consist Avanzato</label>
				<label xml:lang="fr">Addresse UM</label>
				<label xml:lang="de">Verbundadresse</label>
				<label xml:lang="cs">Adresa soupravy</label>
				<label xml:lang="nl">Multitractie-adres</label>
			</variable>
			<!--<variable CV="19" item="ConsistDirection"   mask="VXXXXXXX" default="0">
				<xi:include href="http://jmri.org/xml/decoders/parts/enum-NormRev.xml"/>
				<label>Locomotive Direction In Consist</label>
				<label xml:lang="it">Direzione Loco nel Consist</label>
				<label xml:lang="fr">Direction UM</label>
				<label xml:lang="de">Verbund-Richtung</label>
				<label xml:lang="cs">Směr lokomotivy v soupravě</label>
				<label xml:lang="nl">Rijrichting Multitractie</label>
			</variable>-->
			<!-- CV=29 -->
			<xi:include href="http://jmri.org/xml/decoders/nmra/cv29direction.xml"/>
			<variable CV="29" item="Speed Step Mode" mask="XXXXXXVX" default="1">
				<enumVal>
					<enumChoice choice="14 speed step mode">
						<choice xml:lang="de">14 Fahrstufen Modus</choice>
						<choice xml:lang="nl">14 rijstappen modus</choice>
					</enumChoice>
					<enumChoice choice="28 speed step mode">
						<choice>28 speed step mode</choice>
						<choice xml:lang="de">28 Fahrstufen Modus</choice>
						<choice xml:lang="nl">28 rijstappen modus</choice>
					</enumChoice>
				</enumVal>
				<label>Speed Steps</label>
				<label xml:lang="de">Fahrstufen</label>
				<label xml:lang="nl">Rij stappen</label>
				<tooltip>Sets the Speed Step mode in</tooltip>
				<tooltip xml:lang="de">Setzt den Fahrstufenmodus in</tooltip>
				<tooltip xml:lang="nl">Hiermee stelt u de rijstappenmodus in</tooltip>
			</variable>
			<xi:include href="http://jmri.org/xml/decoders/nmra/cv29analog.xml"/>
			<variable CV="29" item="Speed Table Definition" mask="XXXVXXXX" default="0">
				<enumVal>
					<enumChoice choice="Don't use Speed Table, use Start Voltage (Vstart)">
						<choice>Use bild-in Speed Table </choice>
						<choice xml:lang="de">Interne Geschwindigkeitstabelle verwenden </choice>
						<choice xml:lang="nl">Gebruik interne snelheidstabel </choice>
					</enumChoice>
