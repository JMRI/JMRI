    <choice xml:lang="it">Non si ferma</choice>
    <choice xml:lang="de">Kein Bremsen</choice>
    </enumChoice>
    <enumChoice choice="Stops when Right rail is higher voltage">
    <choice>Stops when Right rail is higher voltage</choice>
