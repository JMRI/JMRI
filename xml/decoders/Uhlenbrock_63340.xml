        <family name="Feedback Module" mfg="Uhlenbrock Elektronik" type="stationary">
            <model model="Feedback Module 2-rail 63340" productID="6334" formFactor="Stationary" comment="LocoNet 8 block feedback module 2-rail">
              <versionCV />
            </model>
        </family>

        <programming direct="no" paged="no" register="no" ops="no">
          <mode>LOCONETLNCVMODE</mode>
        </programming>

