        <functionlabel num="10" lockable="false">Xing Gate Airhorn</functionlabel>
        <functionlabel num="11" lockable="false">Greaser</functionlabel>
        <functionlabel num="12" lockable="false">Safety Blow off</functionlabel>
        </functionlabels>
      </model>
