              <label xml:lang="fr">Adresse Module: </label>
              <label xml:lang="de">Moduladresse: </label>
              <label xml:lang="nl">Module-adres: </label>
              <label>Module Address: </label>
              <tooltip>Enter as decimal value (1-65534)</tooltip>
            </variable>
