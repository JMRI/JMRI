		<version author="peter.brandenburg@t-online.de"
			version="1" lastUpdated="2006/11/20" />

		<!-- version 1 - added CV's for new generation of decoders - Peter -->

<decoder>
	<family name="DCX Series (v.26 up)" mfg="CT Elektronik">
		<model model="DCX74D" numOuts="2" numFns="5"
		maxMotorCurrent="1.6 A" maxTotalCurrent="0.8 A" >
		<versionCV lowVersionID="26" />
			<output name="1" label="White" />
	    	<output name="2" label="Yellow" />
		</model>
		
		<model model="DCX74DV" numOuts="4" numFns="10"
		maxMotorCurrent="1.6 A" maxTotalCurrent="0.8 A" >
		<versionCV lowVersionID="26" />
			<output name="1" label="White" />
	    	<output name="2" label="Yellow" />
	    	<output name="3" label="Brown" />
	    	<output name="4" label="Brown" />
		</model>
		
		<model model="DCX74z" numOuts="4" numFns="10"
		maxMotorCurrent="1 A" maxTotalCurrent="1 A" >
		<versionCV lowVersionID="26" />
			<output name="1" label="White" />
	    	<output name="2" label="Yellow" />
	    	<output name="3" label="Green" />
	    	<output name="4" label="Violett" />
		</model>
		

	</family>
	
	showEmptyPanes="no"

	<programming direct="yes" paged="no" register="no" ops="yes">
	
	</programming>

	<variables>

		<variable label="Primary Address" CV="1" comment="Short address"
						item="Short Address"  default="03">
			<shortAddressVal/>
		</variable>

		<variable label="Vstart" CV="2" default="2" 
				comment="Range 0-252">
			<decVal max="252"/>
		</variable>
		
		<variable label="Acceleration" CV="3" default="1" item="Accel"
				comment="Range 0-255">
			<decVal max="255"/>
		</variable>

		<variable label="Deceleration" CV="4" default="1" item="Decel"
				comment="Range 0-255">
			<decVal max="255"/>
		</variable>
		
		<variable label="Vhigh" CV="5" default="252" 
				comment="Range 0-252">
			<decVal max="252"/>
		</variable>
		
		<variable label="Vmid" CV="6" default="75" 
				comment="Range 0-252">
			<decVal max="252"/>
		</variable>


		<variable label="Decoder Version" CV="7" readOnly="yes"
