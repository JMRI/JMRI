<?xml version="1.0"?>
<!DOCTYPE decoder:decoder-config SYSTEM "DTD/decoder-config.dtd">
<decoder:decoder-config xmlns:decoder="http://jmri.sourceforge.net/xml/decoder">
<decoder:decoder>
	<decoder:id model="All" mfg="NMRA" mfgID="" defnVersion="0"
				comment="Non-reserved CVs listed in NMRA Recommended Practices"></decoder:id>

	<decoder:programming direct="yes" paged="yes" 
	                     register="yes" ops="yes"></decoder:programming>

	<decoder:variables>
		<decoder:variable name="Primary Address" CV="1" mask="VVVVVVVV" comment="Short address">
			<decoder:decVal min="1" max="127"/>
			</decoder:variable>
		<decoder:variable name="Vstart" CV="2"
						comment="A value of 255 corresponds to 100%">
			<decoder:decVal></decoder:decVal>
			</decoder:variable>
		<decoder:variable name="Acceleration Rate" CV="3">
			<decoder:decVal max="31"/>
			</decoder:variable>
		<decoder:variable name="Deceleration Rate" CV="4">
			<decoder:decVal max="31"/>
			</decoder:variable>
		<decoder:variable name="Vhigh" CV="5">
			<decoder:decVal/>
			</decoder:variable>
		<decoder:variable name="Vmid" CV="6">
			<decoder:decVal/>
			</decoder:variable>
		<decoder:variable name="Manufacturer Version No." CV="7" readOnly="yes">
				<decoder:decVal/>
			</decoder:variable>
		<decoder:variable name="Manufacturer ID" CV="8" readOnly="yes">
				<decoder:decVal/>
			</decoder:variable>
		<decoder:variable name="Total PWM Period" CV="9">
				<decoder:decVal/>
			</decoder:variable>
		<decoder:variable name="EMF Feedback Cutout" CV="10">
				<decoder:decVal/>
			</decoder:variable>
		<decoder:variable name="Packet Time-out Value" CV="11">
				<decoder:decVal/>
			</decoder:variable>
		<decoder:variable name="Power Source Conversion" CV="12">
				<decoder:decVal/>
			</decoder:variable>
		<decoder:variable name="Analog Mode Function Status - F1" CV="13" mask="XXXXXXXV">
			<decoder:enumVal>
				<decoder:enumChoice choice="off"/>
				<decoder:enumChoice choice="on"/>
				</decoder:enumVal>
			</decoder:variable>
		<decoder:variable name="Analog Mode Function Status - F2" CV="13" mask="XXXXXXVX">
			<decoder:enumVal>
				<decoder:enumChoice choice="off"/>
				<decoder:enumChoice choice="on"/>
				</decoder:enumVal>
			</decoder:variable>
		<decoder:variable name="Analog Mode Function Status - F3" CV="13" mask="XXXXXVXX">
			<decoder:enumVal>
				<decoder:enumChoice choice="off"/>
				<decoder:enumChoice choice="on"/>
				</decoder:enumVal>
			</decoder:variable>
		<decoder:variable name="Analog Mode Function Status - F4" CV="13" mask="XXXXVXXX">
			<decoder:enumVal>
				<decoder:enumChoice choice="off"/>
				<decoder:enumChoice choice="on"/>
				</decoder:enumVal>
			</decoder:variable>
		<decoder:variable name="Analog Mode Function Status - F5" CV="13" mask="XXXVXXXX">
			<decoder:enumVal>
				<decoder:enumChoice choice="off"/>
				<decoder:enumChoice choice="on"/>
				</decoder:enumVal>
			</decoder:variable>
		<decoder:variable name="Analog Mode Function Status - F6" CV="13" mask="XXVXXXXX">
			<decoder:enumVal>
				<decoder:enumChoice choice="off"/>
				<decoder:enumChoice choice="on"/>
				</decoder:enumVal>
			</decoder:variable>
		<decoder:variable name="Analog Mode Function Status - F7" CV="13" mask="XVXXXXXX">
			<decoder:enumVal>
				<decoder:enumChoice choice="off"/>
				<decoder:enumChoice choice="on"/>
				</decoder:enumVal>
			</decoder:variable>
		<decoder:variable name="Analog Mode Function Status - F8" CV="13" mask="VXXXXXXX">
			<decoder:enumVal>
				<decoder:enumChoice choice="off"/>
				<decoder:enumChoice choice="on"/>
				</decoder:enumVal>
			</decoder:variable>
		<decoder:variable name="Normal direction of motion" CV="29" mask="XXXXXXXV">
			<decoder:enumVal>
				<decoder:enumChoice choice="forward"/>
				<decoder:enumChoice choice="reverse"/>
				</decoder:enumVal>
			</decoder:variable>
		<decoder:variable name="Extended Address" CV="17">
			<decoder:longAddressVal/>
			</decoder:variable>
		<decoder:variable name="Consist Address" CV="19">
				<decoder:decVal/>
			</decoder:variable>
		<decoder:variable name="Consist Address Active For F1" CV="21" mask="XXXXXXXV">
			<decoder:enumVal>
				<decoder:enumChoice choice="Locomotive Address Only"/>
				<decoder:enumChoice choice="Respond to Consist Address"/>
				</decoder:enumVal>
			</decoder:variable>
		<decoder:variable name="Consist Address Active For F2" CV="21" mask="XXXXXXVX">
			<decoder:enumVal>
				<decoder:enumChoice choice="Locomotive Address Only"/>
				<decoder:enumChoice choice="Respond to Consist Address"/>
				</decoder:enumVal>
			</decoder:variable>
		<decoder:variable name="Consist Address Active For F3" CV="21" mask="XXXXXVXX">
			<decoder:enumVal>
				<decoder:enumChoice choice="Locomotive Address Only"/>
				<decoder:enumChoice choice="Respond to Consist Address"/>
				</decoder:enumVal>
			</decoder:variable>
		<decoder:variable name="Consist Address Active For F4" CV="21" mask="XXXXVXXX">
			<decoder:enumVal>
				<decoder:enumChoice choice="Locomotive Address Only"/>
				<decoder:enumChoice choice="Respond to Consist Address"/>
				</decoder:enumVal>
			</decoder:variable>
		<decoder:variable name="Consist Address Active For F5" CV="21" mask="XXXVXXXX">
			<decoder:enumVal>
				<decoder:enumChoice choice="Locomotive Address Only"/>
				<decoder:enumChoice choice="Respond to Consist Address"/>
				</decoder:enumVal>
			</decoder:variable>
		<decoder:variable name="Consist Address Active For F6" CV="21" mask="XXVXXXXX">
			<decoder:enumVal>
				<decoder:enumChoice choice="Locomotive Address Only"/>
				<decoder:enumChoice choice="Respond to Consist Address"/>
				</decoder:enumVal>
			</decoder:variable>
		<decoder:variable name="Consist Address Active For F7" CV="21" mask="XVXXXXXX">
			<decoder:enumVal>
				<decoder:enumChoice choice="Locomotive Address Only"/>
				<decoder:enumChoice choice="Respond to Consist Address"/>
				</decoder:enumVal>
			</decoder:variable>
		<decoder:variable name="Consist Address Active For F8" CV="21" mask="VXXXXXXX">
			<decoder:enumVal>
				<decoder:enumChoice choice="Locomotive Address Only"/>
				<decoder:enumChoice choice="Respond to Consist Address"/>
				</decoder:enumVal>
			</decoder:variable>
		<decoder:variable name="Consist Address Active For FL in Forward" CV="22" mask="XXXXXXXV">
			<decoder:enumVal>
				<decoder:enumChoice choice="Locomotive Address Only"/>
				<decoder:enumChoice choice="Respond to Consist Address"/>
				</decoder:enumVal>
			</decoder:variable>
		<decoder:variable name="Consist Address Active For FL in Reverse" CV="22" mask="XXXXXXVX">
			<decoder:enumVal>
				<decoder:enumChoice choice="Locomotive Address Only"/>
				<decoder:enumChoice choice="Respond to Consist Address"/>
				</decoder:enumVal>
			</decoder:variable>
		<decoder:variable name="Acceleration Adjustment" CV="23" mask="XVVVVVVV">
				<decoder:decVal max="127"/>
			</decoder:variable>
		<decoder:variable name="Acceleration Adjustment Sign" CV="23" mask="VXXXXXXX">
			<decoder:enumVal>
				<decoder:enumChoice choice="Adjustment Added"/>
				<decoder:enumChoice choice="Adjustment Subtracted"/>
				</decoder:enumVal>
			</decoder:variable>
		<decoder:variable name="Deceleration Adjustment" CV="24" mask="XVVVVVVV">
				<decoder:decVal max="127"/>
			</decoder:variable>
		<decoder:variable name="Deceleration Adjustment Sign" CV="24" mask="VXXXXXXX">
			<decoder:enumVal>
				<decoder:enumChoice choice="Adjustment Added"/>
				<decoder:enumChoice choice="Adjustment Subtracted"/>
				</decoder:enumVal>
			</decoder:variable>
		<decoder:variable name="Speed Table/Mid Range Cab Speed Step" CV="24"
				comment="See RP 9.2.2 for more information. Values from 2 to 127 indicate preloaded speed tables. 0 or 1 means not used.  Values between 128 and 154 define the cab throttle position where the mid range decoder speed value is applied">
				<decoder:decVal/>
			</decoder:variable>
		<decoder:variable name="Locomotive Direction" CV="29" mask="XXXXXXXV">
			<decoder:enumVal>
				<decoder:enumChoice choice="normal"/>
				<decoder:enumChoice choice="reversed"/>
				</decoder:enumVal>
			</decoder:variable>
		<decoder:variable name="FL Location" CV="29" mask="XXXXXXVX">
			<decoder:enumVal>
				<decoder:enumChoice choice="Bit 4 in speed and sirection instructions controls FL (14 speed steps)"/>
				<decoder:enumChoice choice="Bit 4 in function group 1 instruction controls FL (28 or 128 speed steps)"/>
				</decoder:enumVal>
			</decoder:variable>
		<decoder:variable name="Power source conversion" CV="29" mask="XXXXXVXX">
			<decoder:enumVal>
				<decoder:enumChoice choice="NMRA Digital only"/>
				<decoder:enumChoice choice="Power source conversion enabled"/>
				</decoder:enumVal>
			</decoder:variable>
		<decoder:variable name="Advanced Decoder Acknowledgement" CV="29" mask="XXXXVXXXX">
			<decoder:enumVal>
				<decoder:enumChoice choice="disabled"/>
				<decoder:enumChoice choice="enabled"/>
				</decoder:enumVal>
			</decoder:variable>
		<decoder:variable name="Speed table" CV="29" mask="XXXVXXXX">
			<decoder:enumVal>
				<decoder:enumChoice choice="Set by CVs 2, 5 and 6"/>
				<decoder:enumChoice choice="Set by CVs 66 through 95"/>
				</decoder:enumVal>
			</decoder:variable>
		<decoder:variable name="Kick Start" CV="65">
			<decoder:decVal/>
			</decoder:variable>
		<decoder:variable name="Forward Trim" CV="66">
			<decoder:decVal/>
			</decoder:variable>
		<decoder:variable name="Reverse Trim" CV="95">
			<decoder:decVal/>
			</decoder:variable>
		<decoder:variable name="User identification #1" CV="105">
			<decoder:decVal/>
			</decoder:variable>
		<decoder:variable name="User identification ID #2" CV="106">
			<decoder:decVal/>
			</decoder:variable>
		<decoder:variable name="Speed Table" CV="67">
			<decoder:speedTableVal/>
			</decoder:variable>
	</decoder:variables>
	
</decoder:decoder>
</decoder:decoder-config>
