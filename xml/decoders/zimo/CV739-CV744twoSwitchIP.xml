<?xml version="1.0" encoding="utf-8"?>
<?xml-stylesheet type="text/xsl" href="../XSLT/decoder.xsl"?>
<!-- version 1 - for Unified Software -->
<!-- Nigel Cliffe, 16th Nov 2012 -->
<!-- From Version 32 firmware -->
<!-- CV's 739-768 control volume and sound choices of individual sounds -->
<!-- manual hints that control of sound in slot, looping etc. is possible, but gives no details -->
<!-- see Zimo German manual for small decoders, -->
<variables xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://jmri.org/xml/schema/decoder.xsd">
  <!-- include this CV to put it onto the Sound Levels pane -->
  <variable CV="739" item="Zimo Sound Setting 51">
    <decVal/>
    <label>Switch triggered sound S1</label>
  </variable>
  <variable CV="741" item="Zimo Sound Setting 52">
    <decVal/>
    <label>Switch triggered sound S2</label>
  </variable>
  <!-- removed as only two switch inputs in this version of file... 
	  <variable label="Switch triggered sound S3" CV="743" item="Zimo Sound Setting 53">
        <decVal/>
      </variable>
	  -->
</variables>
