  <variable CV="178" item="Analog Vstart" default="0">
    <decVal min="0" max="255"/>
    <label>Analog Vstart</label>
    <label xml:lang="de">Anfahrspannung Analog</label>
  </variable>
  <variable CV="179" item="Analog Vhigh" default="0">
    <decVal min="0" max="255"/>
    <label>Analog Vhigh</label>
    <label xml:lang="de">Geschwindigkeit Analog</label>
  </variable>
  <variable CV="273" item="Delayed start after blow-off time" default="0">
    <decVal min="0" max="255"/>
    <label>Delayed start after blow-off time</label>
    <label xml:lang="it">Ritardo partenza dopo Scarico Vapore</label>
    <label xml:lang="de">Entwässerungs-Anfahrverzögerung</label>
    <label xml:lang="cs">Zpoždění rozjezdu po otevření odvodňovacího ventilu</label>
    <tooltip>0 = no delay, 1 = blow-off on speed step 1, 2+ = 10ths of seconds, INC=1</tooltip>
    <tooltip xml:lang="de">0 = keine Verzögerung, 1 = Entwässern per Fahrregler, 2+ = Anfahrverzögerung in Zehntel-sec, INC=1, Angleichung des Anfahrens im Traktionsbetrieb</tooltip>
  </variable>
  <variable CV="274" item="Blow-off schedule" default="30">
    <decVal min="0" max="255"/>
    <label>Blow-off schedule</label>
    <label xml:lang="it">Pianificazione Scarico Vapore</label>
    <label xml:lang="de">Entwässerung-Stillstandzeit</label>
    <label xml:lang="cs">Časový rozvrh odvodňovacího ventilu</label>
    <tooltip>Blow-off suppressed by shunting, 10ths of seconds, INC=10</tooltip>
    <tooltip xml:lang="de">Stillstandzeit im Rangierbetrieb, INC=10</tooltip>
  </variable>
