      <!-- Output Mapping CV35 - 42 -->
      <!-- When Function high = 128 then Function low has the DCC range of F0 - F127 -->
      <variable item="Output A1 Mapping" CV="35" default="1" mask="VXXXXXXX" include="76900V7,PIKO56126">
        <xi:include href="http://jmri.org/xml/decoders/parts/enum-OnOff.xml"/>
        <label>Map to F0-F28</label>
        <tooltip>Turn off to use Binary State control</tooltip>
        <tooltip xml:lang="nl">Schakel uit om BSC te gebruiken</tooltip>
      </variable>
      <variable item="A1 BSC" CV="36" default="0">
        <splitVal highCV="35" min="0" max="32767" upperMask="XVVVVVVV"/>
        <label> BSC:</label>
        <tooltip>With Binary State Control (BSC) function numbers 0-32767 can be used</tooltip>
        <tooltip xml:lang="nl">Met Binary State Control (BSC) kun je functienummers 0-32767 gebruiken</tooltip>
      </variable>
