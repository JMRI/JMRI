        <label xml:lang="it">Volt Intermedi</label>
       <label xml:lang="fr">Vmoy</label>
       <label xml:lang="de">Mittengeschwindigkeit</label>
      </variable>
      <variable CV="7" readOnly="yes" item="Decoder Version" default="104">
        <decVal/>
