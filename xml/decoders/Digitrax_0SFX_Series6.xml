          <functionlabel num="7" lockable="true">Notch DOWN decel./Steam Blowdown, vol at load</functionlabel>
          <functionlabel num="8" lockable="true">Mute (full-half-silent)</functionlabel>
          <functionlabel num="9" lockable="true">Brake squeal (eStop wF8)</functionlabel>
          <functionlabel num="10" lockable="true">Xing Gate Airhorn/Whistle</functionlabel>   <!-- Xing Gate Airhorn wBell / Steam Xing Gate Whistle wBell -->
          <functionlabel num="11" lockable="true">UNcoupling</functionlabel>
          <functionlabel num="12" lockable="false">Wheel Squeal</functionlabel>
          <functionlabel num="13" lockable="true">Air feat. disabled/Steam Coal Fill</functionlabel>
          <functionlabel num="14" lockable="true">Emerg. Stop Shutdown</functionlabel>
          <functionlabel num="15" lockable="true">Sander/Steam Water Tank</functionlabel>
          <functionlabel num="16" lockable="true">Steam Coal Stoker/Coal Shovel</functionlabel>
          <functionlabel num="17" lockable="true">Boxcar Boom</functionlabel>
          <functionlabel num="18" lockable="true">High Fan/Steam Greaser</functionlabel>
          <functionlabel num="19" lockable="true">AUX gen./Steam Coal Grate Shaker</functionlabel>
          <functionlabel num="20" lockable="false">Misc. User Sound/Steam SANDER, Manual Chuff</functionlabel> <!-- Steam SANDER, Manual Chuff 0 (0=CylOpn) -->
          <functionlabel num="21" lockable="false">Select Notch 1</functionlabel>
          <functionlabel num="22" lockable="false">Select Notch 2</functionlabel>
          <functionlabel num="23" lockable="false">Select Notch 3</functionlabel>
          <functionlabel num="24" lockable="false">Select Notch 4</functionlabel>
          <functionlabel num="25" lockable="false">Select Notch 5</functionlabel>
          <functionlabel num="26" lockable="false">Select Notch 6</functionlabel>
          <functionlabel num="27" lockable="false">Select Notch 7</functionlabel>
          <functionlabel num="28" lockable="false">Select Notch 8</functionlabel>
        </functionlabels>
      </model>
      <model model="Extended Series 6 Steam" numOuts="6" numFns="28" maxMotorCurrent="1.0A (peak=2A)" formFactor="HO" connector="9pin" productID="SDXH166DExtS" comment="SDXH166D Extended Steam Sound projects v3.5 by John McMasters">
        <output name="1" label="White" connection="wire"/>
        <output name="2" label="Yellow" connection="wire"/>
        <output name="3" label="Green" connection="wire"/>
        <output name="4" label="Violet" connection="wire"/>
        <output name="5" label="F3-Brown" connection="wire"/>
        <output name="6" label="F4-Yellow" connection="wire"/>
        <size length="1.28" width="0.67" height="0.25" units="inches"/>
        <functionlabels>
          <functionlabel num="2" lockable="false">Whistle</functionlabel>
          <functionlabel num="3" lockable="true">Coupler on F3 release / Auto Directional Toots</functionlabel>
          <functionlabel num="4" lockable="true">Open Cylinder Cocks / ( light function (turn on rear light, etc.)</functionlabel>
          <functionlabel num="5" lockable="true">Steam Waterpump turbine</functionlabel> <!--  Dyn. Brakes Fans level 1-3 (handbrake spd=0) -->
          <functionlabel num="6" lockable="true">Steam Safety Blowoff, drier, vol at coast</functionlabel>
          <functionlabel num="7" lockable="true">Steam Blowdown, vol at load</functionlabel>
          <functionlabel num="8" lockable="true">Mute (full-half-silent) [eStop with F9]</functionlabel>
          <functionlabel num="9" lockable="true">Brakes [eStop wF8]</functionlabel>
          <functionlabel num="10" lockable="true">Xing Gate Whistle / Bells</functionlabel>   <!-- Xing Gate Whistle wBell / Steam Xing Gate Whistle wBell -->
          <functionlabel num="11" lockable="true">UNcoupling [disables toots on F3]</functionlabel>
          <functionlabel num="12" lockable="false">Wheel Squeal [disables bell on F6]</functionlabel>
          <functionlabel num="13" lockable="true">Coal Fill</functionlabel>
          <functionlabel num="14" lockable="true">Emerg. Stop Shutdown [disables motor movement when stopped]</functionlabel>
          <functionlabel num="15" lockable="true">Water Tank</functionlabel>
          <functionlabel num="16" lockable="true">Steam Coal Stoker/Coal Shovel</functionlabel>
          <functionlabel num="17" lockable="true">Boxcar Boom</functionlabel>
          <functionlabel num="18" lockable="true">Steam Greaser</functionlabel>
          <functionlabel num="19" lockable="true">Coal Grate Shaker / AshPan Empty</functionlabel>
          <functionlabel num="20" lockable="false">SANDER / Manual Chuff 0</functionlabel> <!-- Steam SANDER, Manual Chuff 0 (0=CylOpn) -->
          <functionlabel num="21" lockable="false">Manual Chuff 1</functionlabel>
          <functionlabel num="22" lockable="false">Manual Chuff 2</functionlabel>
          <functionlabel num="23" lockable="false">Manual Chuff 3</functionlabel>
          <functionlabel num="24" lockable="false">Manual Chuff 4</functionlabel>
          <functionlabel num="25" lockable="false">Manual Chuff 5</functionlabel>
          <functionlabel num="26" lockable="false">Wheel Slip on start-up</functionlabel>
          <functionlabel num="27" lockable="false">Slow Start Chuffs (Override wheel slip)</functionlabel>
          <functionlabel num="28" lockable="false">AIR PUMP</functionlabel>
        </functionlabels>
      </model>
      <model model="Extended Series 6 Diesel" numOuts="6" numFns="28" maxMotorCurrent="1.0A (peak=2A)" formFactor="HO" connector="9pin" productID="SDXH166DExtD" comment="SDXH166D Extended Diesel Sound projects v3.5 by John McMasters">
