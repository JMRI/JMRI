        <output name="4" label="Violet" connection="wire"/>
        <output name="5" label="F3-Brown" connection="wire"/>
        <output name="6" label="F4-Yellow" connection="wire"/>
        <size length="1.28" width="0.67" height="0.25" units="inches"/>
        <functionlabels>
          <functionlabel num="2" lockable="false">Horn</functionlabel>
          <functionlabel num="3" lockable="true">Coupler on F3 release / Auto Directional Toots</functionlabel>
          <functionlabel num="4" lockable="true">Reserved for light func.</functionlabel>
          <functionlabel num="5" lockable="true">Dyn. Brakes Fans lev.1-3 / Handbrake</functionlabel> <!--  Dyn. Brakes Fans level 1-3 (handbrake spd=0) -->
          <functionlabel num="6" lockable="true">Notch UP accel. /  vol at coast</functionlabel>
          <functionlabel num="7" lockable="true">Notch DOWN decel. / vol at load</functionlabel>
          <functionlabel num="8" lockable="true">Mute (full-half-silent) [eStop with F9]</functionlabel>
          <functionlabel num="9" lockable="true">Brake squeal (eStop wF8)</functionlabel>
          <functionlabel num="10" lockable="true">Xing Gate Airhorn / Bells</functionlabel>   <!-- Xing Gate Airhorn wBell / Steam Xing Gate Whistle wBell -->
          <functionlabel num="11" lockable="true">UNcoupling [disables toots on F3]</functionlabel>
          <functionlabel num="12" lockable="false">Wheel Squeal [disables bell on F6]</functionlabel>
          <functionlabel num="13" lockable="true">Air feat. disabled</functionlabel>
          <functionlabel num="14" lockable="true">Emerg. Stop Shutdown [disables motor movement when stopped]</functionlabel>
          <functionlabel num="15" lockable="true">Sander</functionlabel>
          <functionlabel num="16" lockable="true">na</functionlabel>
          <functionlabel num="17" lockable="true">Boxcar Boom</functionlabel>
          <functionlabel num="18" lockable="true">High Fan</functionlabel>
          <functionlabel num="19" lockable="true">AUX gen./ HEP mode</functionlabel>
          <functionlabel num="20" lockable="false">Misc. User Sound</functionlabel> <!-- Steam SANDER, Manual Chuff 0 (0=CylOpn) -->
