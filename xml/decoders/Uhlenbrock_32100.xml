          <enumChoice choice="Range I (CVs 900-939)" value="1">
            <choice>Range I (CVs 900-939)</choice>
            <choice xml:lang="de">Bereich I (CVs 900-939)</choice>
          </enumChoice>
          <enumChoice choice="Range II (CVs 940-979)">
