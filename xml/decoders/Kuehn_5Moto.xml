<variable label="I adjustment" CV="53" default="2" item="EMF Dynamic Config"
				tooltip="I adjustment, 1-63"
				comment="Range 1-63">
			<decVal min="1" max="63"/>
	</variable>	

<variable label="P adjustment" CV="54" default="16" item="EMF Static Config"
				tooltip="P adjustment, 1-63"
				comment="Range 1-63">
		<decVal min="1" max="63"/>
	</variable>

<variable label="Cycle Time Effects/Output Reduction" CV="55" default="0" item="Output Reduction" mask="XXXXXXXV"
	          		comment="Controls function output voltage
	          				0=full on, 7=lowest">
		<decVal min="0" max="7"/>
	</variable>
	
<variable label="Cycle Time Effects/Output Reduction" CV="55" default="0" item="Cycle Time Effects" mask="XXXXXXVX"
	          		comment="Controls Cycle Time of Effects (CV49/50)
	          				0=0.5s, 9=4.5s">
		<decVal min="0" max="9"/>
	</variable>

<variable label="User Configurationregister" CV="56" default="0" item="Direction Dependent Light (white/yellow)" mask="XXXXXXXV"
				comment="Direction Depentent Lights or Independant Light and Function on yellow">
		<enumVal>
			<enumChoice choice="On"/>
			<enumChoice choice="Off"/>
		</enumVal>
	</variable>
<variable label="User Configurationregister" CV="56" default="1" item="BEMF Load Compensation" mask="XXXXXXVX"
				comment="BEMF Load Compensation disabled/enabled">
