            <choice xml:lang="de">3 </choice>
          </enumChoice>
          <enumChoice choice="4 ">
            <choice>4 </choice>
            <choice xml:lang="de">4 </choice>
          </enumChoice>
          <enumChoice choice="5 ">
            <choice>5 </choice>
            <choice xml:lang="de">5 </choice>
