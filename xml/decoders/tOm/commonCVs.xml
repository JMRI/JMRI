<variables xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://jmri.org/xml/schema/decoder.xsd">

      <xi:include href="http://jmri.org/xml/decoders/nmra/shortAndLongAddress.xml"/>

      <variable item="Vstart" CV="2" default="1" comment="Range 1-255">
        <decVal min="1" max="255"/>
        <label>Vstart</label>
      </variable>
      <variable CV="3" default="5" item="Accel" comment="Acceleration Rate 0=Fastest acceleration">
        <decVal min="0" max="63"/>
        <label>Acceleration</label>
      </variable>
      <variable CV="4" default="5" item="Decel" comment="Deceleration Rate 0=Fastest deceleration">
        <decVal min="0" max="63"/>
        <label>Deceleration</label>
      </variable>
      <variable item="Vhigh" CV="5" default="200" comment="Vhigh Range 1-255">
        <decVal min="1" max="255"/>
        <label>Vhigh</label>
      </variable>
      <variable item="Vmid" CV="6" default="60" comment="Vmid, recommended value = [25%-75%] Vhigh">
        <decVal min="0" max="255"/>
        <label>Vmid</label>
      </variable>
      <variable CV="7" item="Decoder Version" readOnly="yes" default="3">
        <decVal/>
        <label>Decoder Version</label>
      </variable>
      <variable CV="8" item="Manufacturer" default="78">
        <decVal/>
        <label>Manufacturer ID</label>
      </variable>
      <variable CV="9" default="3" item="Load Control" comment="Motor Control Algorithm, 0-8 User defined = 9 (see CV60)">
        <enumVal>
          <enumChoice choice="Low inertia 1"  value="0"/>
          <enumChoice choice="Low inertia 2"  value="1"/>
          <enumChoice choice="Low inertia 3"  value="2"/>
          <enumChoice choice="Generic 1"      value="3"/>
          <enumChoice choice="Generic 2"      value="4"/>
          <enumChoice choice="Generic 3"      value="5"/>
          <enumChoice choice="High inertia 1" value="6"/>
          <enumChoice choice="High inertia 2" value="7"/>
          <enumChoice choice="High inertia 3" value="8"/>
          <enumChoice choice="Manual mode"    value="9"/>
        </enumVal>

        <label>Motor Control Algorithm</label>
      </variable>

