      <model model="MX66S version 3/11" lowVersionID="3" highVersionID="11" maxInputVolts="12-24V" maxMotorCurrent="3A, peak=5A for 5 sec." maxTotalCurrent="3A" formFactor="0, LGB, 1, 2" numOuts="8" numFns="12">
