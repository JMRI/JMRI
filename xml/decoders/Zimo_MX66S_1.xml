        <output name="2" label="Rear head lights"/>
        <output name="3" label="Function 2"/>
        <output name="4" label="Function 3"/>
        <output name="5" label="Function 4"/>
        <output name="6" label="Function 5"/>
        <output name="7" label="Function 6"/>
        <output name="8" label="Function 7"/>
