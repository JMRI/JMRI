  <version author="Bob Jacobsen" version="1" lastUpdated="20150812"/>

  <decoder>
    <family name="Hans Deloof LocoIO" mfg="Public-domain and DIY">
      <model model="Basic LocoIO" productID="142">
          <versionCV lowVersionID="134" highVersionID="142"/>
      </model>
    </family>
    <programming direct="no" paged="no" register="no" ops="no">
        <mode>LOCONETSV1MODE</mode>
    </programming>
    <variables>

        <variable CV="100" readOnly="yes" item="Decoder Version" default="142">
            <decVal/>
            <label>PIC Version: </label>
            <label xml:lang="it">Versione PIC: </label>
            <label xml:lang="fr">Version PIC: </label>
            <label xml:lang="de">PIC Version: </label>
            <label xml:lang="nl">PIC-versie: </label>
        </variable>

        <variable CV="0" mask="VVVVXXXX" comment="Board Blink Rate" item="Blink Rate" default="0">
            <decVal min="0" max="15"/>
            <label>Blink Rate:</label>
            <tooltip>Choose from 0 = none to 15 = fast</tooltip>
        </variable>

        <variable CV="0" mask="XXXXVVXX" comment="bits 3-2" item="Action Mode" default="0">
            <enumVal>
                <enumChoice choice="0" value="0">
                    <choice>Normal Ports</choice>
                    <choice xml:lang="nl">Normaal</choice>
                </enumChoice>
                <enumChoice choice="2" value="2">
                    <choice>2-Position Servo</choice>
                </enumChoice>
                <enumChoice choice="3" value="3">
                    <choice>4-Position Servo</choice>
                </enumChoice>
            </enumVal>
            <label>Board Port type:</label>
            <tooltip>Select how the ports on this board should operate</tooltip>
        </variable>

      <variable CV="0" mask="XXXXXXVX" item="Alternate Mode" default="0">
        <xi:include href="http://jmri.org/xml/decoders/parts/enum-NoYes.xml"/>
        <label>Alternate code:</label>
        <tooltip>Button sends alternating code, default unselected = fixed code</tooltip>
      </variable>

      <variable CV="0" mask="XXXXXXXV" item="Port Refresh" default="0">
        <xi:include href="http://jmri.org/xml/decoders/parts/enum-NoYes.xml"/>
        <label>Port Refresh:</label>
        <tooltip>Refresh Ports, default unselected</tooltip>
      </variable>

     <!-- This sets a single JMRI-style composite address in two CVs -->
      <variable CV="1" item="Short Address" comment="Board SV address" default="81">
        <splitVal highCV="2" factor="1" offset="-256" default="1"/>
        <label>Board Address (14 bit):</label>
        <tooltip>This provides the full address by combining the LocoIO Address upper and LocoIO Subaddress lower parts</tooltip>
      </variable>

      <!-- This repurposes the UserID fields on the Basic pane to show -->
      <!-- the individual address bytes.                               -->
      <!-- Note defaults must be present both locations and agree      -->
      <variable item="User Id #1" CV="1" default="1">
        <decVal max="127"/>
            <label>/ SubAddress (7-bit low):</label>
            <tooltip>Message DSTL byte</tooltip>
      </variable>

      <variable item="User Id #2" CV="2" default="81">
        <decVal min="1" max="127"/>
        <label>LocoIO Address (7-bit high):</label>
        <tooltip>Message DSTH byte</tooltip>
      </variable>

      <!-- Per Port config SVs, handled like CVs, uses lists from public_domain.enum-HDL-PortMode -->

      <!-- Port 1 -->
      <variable item="Port1 Mode" CV="5" mask="VVVVXXXX">
        <!--<splitVal highCV="3" factor="1" offset="0" upperMask="VVVVVVVV"/>-->
        <xi:include href="http://jmri.org/xml/decoders/public_domain/enum-HDL-PortMode.xml"/>
        <label>Mode:</label>
        <label xml:lang="nl">Functie:</label>
        <tooltip>Enter code from list</tooltip>
      </variable>

      <variable item="Port1 Address" CV="4" mask="XVVVVVVV">
        <!-- splitVal misses bit 8 in port address -->
        <splitVal highCV="5" factor="128" offset="-1" upperMask="XXXXVVVV" max="4096"/>
        <label>Port Address (dec):</label>
        <label xml:lang="nl">Poortadres (dec):</label>
      </variable>

      <variable item="Port1 Config" CV="3">
        <decVal min="7" max="208"/>
        <label>Config (dec):</label>
      </variable>

      <variable item="Port1 Value1" CV="4">
        <decVal/>
        <label>Value1 (dec):</label>
      </variable>

      <variable item="Port1 Value2" CV="5">
        <decVal min="0" max="112"/>
        <label>Value2 (dec):</label>
      </variable>

      <!-- Port 2 -->
      <variable item="Port2 Mode" CV="8" mask="VVVVXXXX">
        <!--<splitVal highCV="6" factor="1" offset="0" upperMask="VVVVVVVV"/>-->
        <xi:include href="http://jmri.org/xml/decoders/public_domain/enum-HDL-PortMode.xml"/>
        <label>Mode:</label>
        <label xml:lang="nl">Functie:</label>
        <tooltip>Enter code from list</tooltip>
      </variable>

      <variable item="Port2 Address" CV="7" mask="XVVVVVVV">
        <!-- splitVal misses bit 8 in port address -->
        <splitVal highCV="8" factor="128" offset="-1" upperMask="XXXXVVVV" max="4096"/>
        <label>Port Address (dec):</label>
        <label xml:lang="nl">Poortadres (dec):</label>
      </variable>

      <variable item="Port2 Config" CV="6">
        <decVal min="7" max="208"/>
        <label>Config (dec):</label>
      </variable>

      <variable item="Port2 Value1" CV="7">
        <decVal/>
        <label>Value1 (dec):</label>
      </variable>

      <variable item="Port2 Value2" CV="8">
        <decVal min="0" max="112"/>
        <label>Value2 (dec):</label>
      </variable>

      <!-- Port 3 -->
      <variable item="Port3 Mode" CV="11" mask="VVVVXXXX">
        <!--<splitVal highCV="9" factor="1" offset="0" upperMask="VVVVVVVV"/>-->
        <xi:include href="http://jmri.org/xml/decoders/public_domain/enum-HDL-PortMode.xml"/>
        <label>Mode:</label>
        <label xml:lang="nl">Functie:</label>
        <tooltip>Enter code from list</tooltip>
      </variable>

      <variable item="Port3 Address" CV="10" mask="XVVVVVVV">
        <!-- splitVal misses bit 8 in port address -->
        <splitVal highCV="11" factor="128" offset="-1" upperMask="XXXXVVVV" max="4096"/>
        <label>Port Address (dec):</label>
        <label xml:lang="nl">Poortadres (dec):</label>
      </variable>

      <variable item="Port3 Config" CV="9">
        <decVal min="10" max="208"/>
        <label>Config (dec):</label>
      </variable>

      <variable item="Port3 Value1" CV="10">
        <decVal/>
        <label>Value1 (dec):</label>
      </variable>

      <variable item="Port3 Value2" CV="11">
        <decVal min="0" max="112"/>
        <label>Value2 (dec):</label>
      </variable>

      <!-- Port 4 -->
      <variable item="Port4 Mode" CV="14" mask="VVVVXXXX">
        <!--<splitVal highCV="12" factor="1" offset="0" upperMask="VVVVVVVV"/>-->
        <xi:include href="http://jmri.org/xml/decoders/public_domain/enum-HDL-PortMode.xml"/>
        <label>Mode:</label>
        <label xml:lang="nl">Functie:</label>
        <tooltip>Enter code from list</tooltip>
      </variable>

      <variable item="Port4 Address" CV="13" mask="XVVVVVVV">
        <!-- splitVal misses bit 8 in port address -->
        <splitVal highCV="14" factor="128" offset="-1" upperMask="XXXXVVVV" max="4096"/>
        <label>Port Address (dec):</label>
        <label xml:lang="nl">Poortadres (dec):</label>
      </variable>

      <variable item="Port4 Config" CV="12">
        <decVal min="7" max="208"/>
        <label>Config (dec):</label>
      </variable>

      <variable item="Port4 Value1" CV="13">
        <decVal/>
        <label>Value1 (dec):</label>
      </variable>

      <variable item="Port4 Value2" CV="14">
        <decVal min="0" max="112"/>
        <label>Value2 (dec):</label>
      </variable>

