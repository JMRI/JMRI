  <!-- Specific for Qdecoder accessory decoder -->
	<!-- Equivalent to CV117, CV127, ..., CV187	for A0 to A8 		-->
	<!-- For compatibility purpose with other decoders					-->
 	<variable item="On Time A0-A1" CV="3" default="0" minOut="2">
		<decVal/>
    <label>On Time A0-A1</label>
    <label xml:lang="de">An-zeit A0-A1</label>
    <tooltip>in 1/100 seconds</tooltip>
    <tooltip xml:lang="de">in 1/100 Sekunden</tooltip>
  </variable>
 	<variable item="On Time A2-A3" CV="4" default="0" minOut="4">
		<decVal/>
    <label>On Time A2-A3</label>
    <label xml:lang="de">An-zeit A2-A3</label>
    <tooltip>in 1/100 seconds</tooltip>
    <tooltip xml:lang="de">in 1/100 Sekunden</tooltip>
  </variable>
 	<variable item="On Time A4-A5" CV="5" default="0" minOut="6">
		<decVal/>
    <label>On Time A4-A5</label>
    <label xml:lang="de">An-zeit A4-A5</label>
    <tooltip>in 1/100 seconds</tooltip>
    <tooltip xml:lang="de">in 1/100 Sekunden</tooltip>
  </variable>
 	<variable item="On Time A6-A7" CV="6" default="0" minOut="8">
		<decVal/>
    <label>On Time A6-A7</label>
    <label xml:lang="de">An-zeit A6-A7</label>
    <tooltip>in 1/100 seconds</tooltip>
    <tooltip xml:lang="de">in 1/100 Sekunden</tooltip>
  </variable>
