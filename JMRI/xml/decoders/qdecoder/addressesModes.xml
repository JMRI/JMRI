  <!-- Specific for Qdecoder accessory decoder -->
	<!-- Fast Mode Reset		 										 -->
	<variable CV="7" writeOnly="yes" item="Fast Mode Reset" default="0">
		<decVal/>
		<label>Fast Mode Reset</label>
		<label xml:lang="de">Schnell Mode Reset</label>
		<tooltip>&lt;html&gt;Input the accessory address of the first article&lt;br&gt;in CV1 and if needed in CV9.&lt;br&gt;Then write the Mode value in CV7.&lt;/html&gt;</tooltip>
		<tooltip xml:lang="de">&lt;html&gt;Legen Sie die Zubehöradresse des ersten Artikels fest&lt;br&gt;und schreiben Sie diese Adresse in die CV1 und gegebenenfalls in die CV9.&lt;br&gt;Schreiben Sie anschließend den Wert für den Mode auf die CV7.&lt;/html&gt;</tooltip>
	</variable>
