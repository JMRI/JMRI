      <variable item="Output 1 behavior" CV="53" mask="XXXXXXVV" default="1">
        <enumVal>
          <enumChoice choice="always on" value="0">
            <choice>always on</choice>
          </enumChoice>
          <enumChoice choice="backward inactive" value="1">
            <choice>backward inactive</choice>
