    <!ATTLIST sensoricon forcecontroloff	(true|false) "false">
    <!ATTLIST sensoricon momentary	(true|false) "false">
    <!ATTLIST sensoricon text	    CDATA #IMPLIED>
    <!ATTLIST sensoricon sxscale	CDATA #IMPLIED>
